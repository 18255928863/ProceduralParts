@PART[proceduralTank*]:FOR[rrRealismOverhaul]:NEEDS[TexturesUnlimited]
{
    MODULE
    {
        name = SSTURecolorGUI
    }

    MODULE
    {
        name = KSPTextureSwitch
        transformName = sides
        sectionName = Sides

        currentTextureSet = Stockalike
        textureSet = Original
        textureSet = Tiles
        textureSet = Stockalike
        textureSet = German
        textureSet = Saturn
        textureSet = SegmentedSRB
        textureSet = CryogenicOrange
        textureSet = RedstoneStripes
        textureSet = TitanStripes
        textureSet = WhiteSide
        textureSet = BlueSide
        textureSet = GreySide
        textureSet = Ablative
        textureSet = PlainWhite
        textureSet = SoyuzGreen
        textureSet = Atlas
        textureSet = Mu
        textureSet = Copernicus
        textureSet = GoldFoil
    }

    MODULE
    {
        name = KSPTextureSwitch
        transformName = ends
        sectionName = Ends

        currentTextureSet = CustomEnds
        textureSet = CustomEnds
        textureSet = GreySide
        textureSet = PlainWhite
        textureSet = SoyuzGreen
    }
}
