KSP_TEXTURE_SET:NEEDS[TexturesUnlimited]
{
    name = Original
    title = PP-Original
    recolorable = true
    MATERIAL
    {
        shader = TU/Metallic
        texture = _MainTex,         ProceduralParts/Parts/sides
        texture = _BumpMap,         ProceduralParts/Parts/sides_NRM
//        texture = _MetallicGlossMap,JoeTest/Textures/Smooth-MET
        texture = _MaskTex,         ProceduralParts/Textures/Masks/Solid-MASK
        keyword = TU_RECOLOR
        vector = _DiffuseNorm,      0.5,0.5,0.5
        vector = _MetalNorm,        0.5,0.5,0.5
        vector = _SmoothnessNorm,   0.5,0.5,0.5
        PROPERTY
        {
            name - _MainTex
            scale = 4, 1
        }
        //textureScale = _MainTex, 4, 1
        float = _Metal, 0.2
        float = _Smoothness, 0.2
    }
    COLORS
    {
        mainColor = stockWhite
        secondColor = white
        detailColor = white
    }
    EXTRA_INFO
    {
        author = Ancient Gammoner
        specular = 0.2, 0.2, 0.2
    }
}

